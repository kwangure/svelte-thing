{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"composite": true,
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"inlineSourceMap": true,
		"isolatedModules": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext",
		"verbatimModuleSyntax": true
	}
}
