{
	"name": "@svelte-thing/skinless-components",
	"version": "0.0.5",
	"scripts": {
		"build": "wireit",
		"check": "wireit",
		"pre-commit": "wireit",
		"publint": "wireit"
	},
	"wireit": {
		"build": {
			"command": "svelte-package -i ./src",
			"files": [
				"src/**/*.{js,svelte,ts}",
				"tsconfig.json",
				"../../config/tsconfig.base.json"
			],
			"output": [
				"types"
			]
		},
		"check": {
			"command": "svelte-check --tsconfig ./tsconfig.json",
			"files": [
				"{src,test}/**/*.{js,ts,svelte}",
				"tsconfig.json",
				"../../config/tsconfig.base.json"
			],
			"output": []
		},
		"publint": {
			"command": "publint",
			"dependencies": [
				"build"
			],
			"files": [
				"src/**",
				"types/**",
				"package.json"
			],
			"output": []
		},
		"pre-commit": {
			"dependencies": [
				"build",
				"check",
				"publint"
			]
		},
		"release": {
			"dependencies": [
				"build",
				"check",
				"publint"
			]
		}
	},
	"exports": {
		"./combobox": {
			"types": "./dist/combobox/index.d.ts",
			"svelte": "./dist/combobox/index.js",
			"default": "./dist/combobox/index.js"
		}
	},
	"files": [
		"dist",
		"!**/*.test.js",
		"!**/*.test.js.map",
		"!**/*.test.d.ts"
	],
	"dependencies": {
		"@svelte-thing/builders": "^0.0.5",
		"@svelte-thing/component-utils": "^0.0.2",
		"@svelte-thing/components": "^0.0.31"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.3.5",
		"@types/node": "^20.10.5",
		"publint": "^0.2.6",
		"svelte": "^5.0.0-next.260",
		"svelte-check": "^4.0.0",
		"typescript": "^5.4.5"
	},
	"peerDependencies": {
		"svelte": "^5.0.0-next.260"
	},
	"type": "module"
}
